<% if orderable.buyable.is_a? Dish %>
	<tr>
		<%= render 'dish_orderable', dish: orderable.buyable, orderable: orderable %>
	</tr>
<% elsif orderable.buyable.is_a? MilkteaOrderable %>
	<tr>
		<%= render 'milktea_orderable', orderable: orderable, milktea: orderable.buyable.milktea %>
	</tr>
<% end %>